%file: test.ned

import inet.queueing.source.PassivePacketSource;
import inet.queueing.sink.ActivePacketSink;
import inet.queueing.queue.PacketQueue;
import inet.queueing.tokengenerator.QueueBasedTokenGenerator;
import inet.queueing.server.TokenBasedServer;

network TestPacketQueue
{
    submodules:
        provider: PassivePacketSource {
            @display("p=100,100");
        }
        server: TokenBasedServer {
            @display("p=100,200");
        }
        queue: PacketQueue {
            @display("p=100,300");
        }
        collector: ActivePacketSink {
            @display("p=100,400");
        }
        tokenGenerator: QueueBasedTokenGenerator {
            @display("p=300,100");
        }
    connections allowunconnected:
        provider.out --> server.in;
        server.out --> queue.in;
        queue.out --> collector.in;
}

%inifile: omnetpp.ini

[General]
network = TestPacketQueue
sim-time-limit = 10s
cmdenv-event-banners = false
*.provider.packetLength = 1B
*.provider.providingInterval = 0s
*.server.initialNumTokens = 1
*.collector.collectionInterval = uniform(0s, 2s)
*.tokenGenerator.queueModule = "^.queue"
*.tokenGenerator.serverModule = "^.server"

